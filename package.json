{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@phosphor-icons/react": "^2.0.13",
        "@reduxjs/toolkit": "^1.9.6",
        "date-fns": "^2.30.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.1.2",
        "react-router-dom": "^6.16.0",
        "react-scripts": "^5.0.1",
        "sass": "^1.68.0"
    },
    "scripts": {
        "start": "react-scripts start PORT=3600",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "server": "json-server --watch ./data/db.json --port 3500",
        "dev-server": "json-server --watch ./data/sample_db.json --port 3500 -m ./node_modules/json-server-auth",
        "serve": "concurrently \"npm start\" \"npm run dev-server\""
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "concurrently": "^8.2.1",
        "json-server-auth": "^2.1.0"
    }
}
